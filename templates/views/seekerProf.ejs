      <h3 class="known"><b>Seeker Profile</b></h3>
   
            <div class="card">
  <div class="body">
    <div class="details-row">
      <div class="detail">
        <h4 class="fs-5 fw-bold">Name</h4>
        <p><%=seeker["fullname"]%></p>
      </div>
      <div class="detail">
        <h4 class="fs-5 fw-bold">Email</h4>
        <p><%=seeker["email"]%></p>
      </div>
    </div>
    <div class="details-row">
      <div class="detail">
        <h4 class="fs-5 fw-bold">Phone No</h4>
        <p><%=seeker["phoneno"]%></p>
      </div>
      <div class="detail">
        <h4 class="fs-5 fw-bold">Rating</h4>
        <div class="rating">
          <% var arr =  seeker["rating"]; var rat = 0; for(let g = 0 ; g < arr.length;g++){ rat += arr[g] } rat = rat/arr.length; rat = Math.ceil(rat)%>
          <% for(let i = 1; i <= 5; i++) { %>
              <% if (i <= rat) { %>
                  <span class="star">&#9733;</span>
              <% } else { %>
                  <span class="star gray">&#9733;</span>
              <% } %>
          <% } %>
      </div>
      </div>
    </div>
    <div class="details-row">
      <div class="detail">
        <h4 class="fs-5 fw-bold">Achieved Services</h4>
        
        <p><%=comp%></p>
        
      </div>
      <div class="detail">
        <h4 class="fs-5 fw-bold">Active Services</h4>
        <p><%=inprog%></p>
      </div>
      </div>
      <div class="details">
        <h4 class="fs-5 fw-bold">Address</h4>
        <p><%=seeker["address"]%></p>
      </div>
    
    
    
  </div>
